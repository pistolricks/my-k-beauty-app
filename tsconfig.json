{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "allowJs": true,
    "noEmit": true,
    "strict": false,
    "types": [
      "vinxi/types/client",
      "vite/client"
    ],
    "lib": [
      "DOM.Iterable",
      "EsNext",
      "dom"
    ],
    "isolatedModules": true,
    "paths": {
      "~/*": ["./src/*"]
    }
  }
}
